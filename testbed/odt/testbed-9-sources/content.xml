/text:p></table:table-cell><table:table-cell table:style-name="Tabella1.A2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 2 row 1</text:p></table:table-cell><table:table-cell table:style-name="Tabella1.A2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 3 row 1</text:p></table:table-cell><table:table-cell table:style-name="Tabella1.D2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 4 row 1</text:p></table:table-cell></table:table-row><table:table-row><table:table-cell table:style-name="Tabella1.A2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 1 row 2</text:p></table:table-cell><table:table-cell table:style-name="Tabella1.A2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 2 row 2</text:p></table:table-cell><table:table-cell table:style-name="Tabella1.A2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 3 row 2</text:p></table:table-cell><table:table-cell table:style-name="Tabella1.D2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 4 row 2</text:p></table:table-cell></table:table-row><table:table-row><table:table-cell table:style-name="Tabella1.A2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 1 row 3</text:p></table:table-cell><table:table-cell table:style-name="Tabella1.A2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 2 row 3</text:p></table:table-cell><table:table-cell table:style-name="Tabella1.A2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 3 row 3</text:p></table:table-cell><table:table-cell table:style-name="Tabella1.D2" office:value-type="string"><text:p text:style-name="Table_20_Contents">cell 4 row 3</text:p></table:table-cell></table:table-row></table:table><text:p text:style-name="Table">Tabella <text:sequence text:ref-name="refTable0" text:name="Table" text:formula